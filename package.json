{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@shined/react-use": "^1.11.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "date-fns": "^4.1.0",
    "format-time": "^1.0.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "timers-browserify": "^2.0.12",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.26.10",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "3.5.3",
    "webpack-cli": "^6.0.1"
  },
  "lint-staged": {
    "*.js,jsx": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "react",
      "react-hooks",
      "jsx-a11y",
      "import"
    ],
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier"
    ],
    "rules": {
      "react/jsx-no-target-blank": "off"
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "parser": "@babel/eslint-parser",
    "globals": {
      "node": "readonly",
      "browser": "readonly"
    },
    "settings": {
      "react": {
        "version": "18.3"
      }
    }
  }
}
